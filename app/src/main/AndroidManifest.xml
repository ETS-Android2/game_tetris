<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="msh_TETR.my_work.tetr_by_msh">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="msh_TETR.my_work.tetr_by_msh.AboutDevelopersActivity">
            <intent-filter>
                <action android:name=".AboutDevelopersActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="msh_TETR.my_work.tetr_by_msh.TermsRulesOfGameActivity">
            <intent-filter>
                <action android:name=".TermsRulesOfGameActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="msh_TETR.my_work.tetr_by_msh.FieldActivity">
            <intent-filter>
                <action android:name=".FieldActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="msh_TETR.my_work.tetr_by_msh.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>